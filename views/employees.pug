extends index.pug

block content
    -url = '/employees/create';
     button: a(href=url) Add Employee<br>

    ul
        each employee in emps
            - url = `/employees/${employee.id}`;
            li(class="lists"): a(href=url) Name: #{employee.first_name} #{employee.last_name}<br>Department: #{employee.Department.name}<br> Employee Id: #{employee.id}
